@model IEnumerable<eDietetyk.UserMeals>

<div class="till" id="meals">
    <h2>Twoje posiłki</h2>
    <article>
        <table class="table">
            <tr>
                <th>
                    Nazwa
                </th>
                <th>
                   Data dodania
                </th>
            </tr>

            @if (Model.Any())
            {
                foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @item.Meals.Name
                        </td>
                        <td>
                            @item.CreateDate.ToShortTimeString()
                        </td>
                    </tr>
                }
            }

        </table>
    </article>
    <aside>
        <a href="/UserMeals/Add" class="btn btn-primary">Dodaj</a>
    </aside>
</div>
