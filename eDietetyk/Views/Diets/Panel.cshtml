@model eDietetyk.Models.DietsViewModel
<div class="till" id="diets">
    <h2>Dieta</h2>
    <article>
        <p>
            <label>Kalorie zalecane na dziś</label>
            <span>@(Model.TargetCalories) kcal</span>
        </p>
        <p>
            <label>Kalorie spożyte: </label>
            <span>@(Model.CurrentCalories) kcal</span>
        </p>
        <p>
            <label>Twoje BMI: </label>
            <span>@(Model.Bmi) </span>
        </p>
    </article>
</div>